{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "developer": {
      "via": "projects",
      "plugin": "users-permissions",
      "model": "user"
    },
    "githubURL": {
      "type": "string"
    },
    "livesiteURL": {
      "type": "string"
    },
    "likeCount": {
      "type": "integer"
    },
    "likes": {
      "via": "project",
      "collection": "like"
    },
    "featuredImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
